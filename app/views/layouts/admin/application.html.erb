<!DOCTYPE html>
<html>
<head>
  <title>Dreamyourweb | Admin</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
	<script type="text/javascript" src="/wymeditor/jquery.wymeditor.pack.js">
	</script>
	<script type="text/javascript">
		jQuery(function() {
	    	jQuery(".wymeditor").wymeditor();
		});
	</script>
</head>
<body>
	<%= render 'layouts/header' %>

	<div style="clear:both"></div>

	<% flash.each do |key, value| %>
 		<%= content_tag(:div, value, :class => "site-flash #{key}") %>
	<% end %>

	<%= render "strip" %>

	<div class="content">
		<div id="admin-content">
			<div id="admin-container">
				<%= yield %>
			</div>
		</div>
	</div>
</html>
